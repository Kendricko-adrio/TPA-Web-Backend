type FriendsDetail{
    user1: User!
    user1ID: Int!
    user2: User!
    user2ID: Int!
    friendStatus: FriendStatus!
    friendStatusId: Int!
    CreatedAt: Time!
    UpdatedAt: Time!
    DeletedAt: Time
}


extend type Mutation{
    request(userId1:Int, userId2:Int):FriendsDetail!
}

type FriendStatus{
    friendStatusId: Int!
    statusName:String!
}